#include <stdio.h>

int main() {
    int n, i, max, num;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter %d integers:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &num);
        if (i == 0 || num > max) {
            max = num;
        }
    }

    printf("The largest number is %d\n", max);

    return 0;
}
